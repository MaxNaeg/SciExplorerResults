for plane waves)\\n# Using amplitudes A=0.3 for q=0 and q1, and A=0.3 for Nyquist as well; so amplitude term cancels if g present (same A)\\nomega0 = result['exp_pw_q0_omega_mean']\\nomega1 = result['exp_pw_k1_omega_mean']\\nomegaN = result['exp_pw_nyq_omega_mean']\\n# Solve for J and mu using omega1-omega0 and omegaN-omega0\\n# delta_1 = 2J*(1-cos q1), delta_N = 2J*(1 - cos qN)\\ndelta1 = omega1 - omega0\\ndeltaN = omegaN - omega0\\nJ_est1 = delta1 / (2*(1-np.cos(q1)))\\nJ_estN = deltaN / (2*(1-np.cos(qN)))\\nJ_est = float(np.mean([J_est1, J_estN]))\\nmu_est = float(omega0)\\nresult['J_est_from_planes'] = float(J_est)\\nresult['mu_est_from_planes'] = mu_est\\nresult['J_est_components'] = {'from_q1': float(J_est1), 'from_qN': float(J_estN)}\\n\\n# Norm conservation check across experiments\\nfor label in ['exp_pw_q0','exp_pw_k1','exp_pw_nyq','exp_gauss_low','exp_gauss_high','exp_cos_mod','exp_gauss_ultra','exp_multimode']:\\n    phis = locals()[label+'_phis']\\n    ts = locals()[label+'_ts']\\n    x = locals()[label+'_x']\\n    dx = float(x[1]-x[0])\\n    dens = np.abs(np.array(phis))**2\\n    N_t = dens.sum(axis=1)*dx\\n    result[label+'_norm_mean'] = float(np.mean(N_t))\\n    result[label+'_norm_rel_std'] = float(np.std(N_t)/np.mean(N_t))\\n    result[label+'_norm_rel_drift'] = float((N_t[-1]-N_t[0])/np.mean(N_t))\\n\\n# Gaussian packet diagnostics: center-of-mass and width\\nfor label in ['exp_gauss_low','exp_gauss_high','exp_gauss_ultra']:\\n    phis = np.array(locals()[label+'_phis'])\\n    ts = np.array(locals()[label+'_ts'])\\n    x = np.array(locals()[label+'_x'])\\n    dx = float(x[1]-x[0])\\n    dens = np.abs(phis)**2\\n    N_t = dens.sum(axis=1)*dx\\n    xbar = (dens @ x) * dx / N_t\\n    var = (dens @ (x**2)) * dx / N_t - xbar**2\\n    result[label+'_xbar_std'] = float(np.std(xbar))\\n    result[label+'_width_initial'] = float(np.sqrt(var[0]))\\n    result[label+'_width_final'] = float(np.sqrt(var[-1]))\\n    result[label+'_width_ratio_final_over_initia