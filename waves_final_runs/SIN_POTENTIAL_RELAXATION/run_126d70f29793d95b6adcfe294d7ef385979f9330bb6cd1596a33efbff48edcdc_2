\\n\\n# Plane waves\\nfor label in ['exp_pw_q0','exp_pw_k1','exp_pw_nyq']:\\n    phis = locals()[label+'_phis']\\n    ts = locals()[label+'_ts']\\n    x = locals()[label+'_x']\\n    # pick several indices for robustness\\n    omegas = []\\n    resids = []\\n    for idx in [0, 10, 25, 50, 75]:\\n        om,res = estimate_omega(np.array(phis), np.array(ts), idx)\\n        omegas.append(om)\\n        resids.append(res)\\n    result[label+'_omega_mean'] = float(np.mean(omegas))\\n    result[label+'_omega_std'] = float(np.std(omegas))\\n    result[label+'_phase_resid_mean'] = float(np.mean(resids))\\n    # amplitude stability\\n    amps = np.abs(np.array(phis))\\n    amp_var_t = np.var(amps, axis=1).mean()\\n    amp_mean = amps.mean()\\n    amp_max_change = float(np.max(np.abs(amps - amps[0]))/amp_mean)\\n    result[label+'_amp_mean'] = float(amp_mean)\\n    result[label+'_amp_max_rel_change'] = amp_max_change\\n\\n# Determine wavenumbers used\\nL = exp_pw_k1_x[-1] - exp_pw_k1_x[0]\\ndx = exp_pw_k1_x[1] - exp_pw_k1_x[0]\\nq1 = 2*np.pi/L\\nqN = np.pi/dx\\nresult['q1'] = float(q1)\\nresult['qN'] = float(qN)\\n\\n# Fit tight-binding dispersion: omega(k) = mu + 2J*(1 - cos k) + g*A^2 (for plane waves)\\n# Using amplitudes A=0.3 for q=0 and q1, and A=0.3 for Nyquist as well; so amplitude term cancels if g present (same A)\\nomega0 = result['exp_pw_q0_omega_mean']\\nomega1 = result['exp_pw_k1_omega_mean']\\nomegaN = result['exp_pw_nyq_omega_mean']\\n# Solve for J and mu using omega1-omega0 and omegaN-omega0\\n# delta_1 = 2J*(1-cos q1), delta_N = 2J*(1 - cos qN)\\ndelta1 = omega1 - omega0\\ndeltaN = omegaN - omega0\\nJ_est1 = delta1 / (2*(1-np.cos(q1)))\\nJ_estN = deltaN / (2*(1-np.cos(qN)))\\nJ_est = float(np.mean([J_est1, J_estN]))\\nmu_est = float(omega0)\\nresult['J_est_from_planes'] = float(J_est)\\nresult['mu_est_from_planes'] = mu_est\\nresult['J_est_components'] = {'from_q1': float(J_est1), 'from_qN': float(J_estN)}\\n\\n# Norm conservation check across experiments\\nfo