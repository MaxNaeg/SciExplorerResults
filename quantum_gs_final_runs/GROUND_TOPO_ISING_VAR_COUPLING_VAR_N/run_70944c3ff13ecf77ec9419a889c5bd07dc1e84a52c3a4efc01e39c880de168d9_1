- Sum_nnn (Jx2_0 SxSx) - A K Sum_nnn (SySy - SzSz)\\n\\ndef build_H(N, A, p):\\n    Jx1, Jy1, Jz1, hx, Jx2_0, K = p\\n    Sx, Sy, Sz = site_ops(N)\\n    d = 2**N\\n    H = np.zeros((d,d), dtype=complex)\\n    # NN\\n    for i in range(N-1):\\n        H -= Jx1 * (Sx[i] @ Sx[i+1])\\n        H -= Jy1 * (Sy[i] @ Sy[i+1])\\n        H -= Jz1 * (Sz[i] @ Sz[i+1])\\n    # Field\\n    for i in range(N):\\n        H -= hx * Sx[i]\\n    # NNN\\n    for i in range(N-2):\\n        H -= Jx2_0 * (Sx[i] @ Sx[i+2])\\n        H -= A * K * (Sy[i] @ Sy[i+2] - Sz[i] @ Sz[i+2])\\n    return H, (Sx, Sy, Sz)\\n\\n# Observables per N\\n\\ndef gs_expectations(N, A, p):\\n    H, (Sx, Sy, Sz) = build_H(N, A, p)\\n    vals, vecs = np.linalg.eigh(H)\\n    psi = vecs[:,0]\\n    if N==2:\\n        res = {\\n            'Mx': float(np.real(np.vdot(psi, (Sx[0]+Sx[1]) @ psi))),\\n            'Cxx': float(np.real(np.vdot(psi, (Sx[0]@Sx[1]) @ psi))),\\n            'Cyy': float(np.real(np.vdot(psi, (Sy[0]@Sy[1]) @ psi))),\\n            'Czz': float(np.real(np.vdot(psi, (Sz[0]@Sz[1]) @ psi))),\\n        }\\n        return res\\n    if N==3:\\n        Mx = float(np.real(np.vdot(psi, sum(Sx) @ psi)))\\n        Cxx_nn = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))) for i in range(2))\\n        Cyy_nn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))) for i in range(2))\\n        Czz_nn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))) for i in range(2))\\n        return dict(Mx=Mx, Cxx_nn=Cxx_nn, Cyy_nn=Cyy_nn, Czz_nn=Czz_nn)\\n    if N==4:\\n        Mx = float(np.real(np.vdot(psi, sum(Sx) @ psi)))\\n        Cxx_nn = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))) for i in range(3))\\n        Cyy_nn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))) for i in range(3))\\n        Czz_nn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))) for i in range(3))\\n        Cxx_nnn = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+2]) @ psi))) for i in range(2))\\n        Cyy_nnn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+2]) @ psi))) for i in range(2))\\n        Czz_nnn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+2]) @ psi))) for i in range(2))\\n        return dict(Mx=Mx,Cxx_nn=Cxx_nn,Cyy_nn=Cyy_nn,Czz_nn=Czz_nn,Cxx_nnn=Cxx_nnn,Cyy_nnn=Cyy_nnn,Czz_nnn=Czz_nnn)\\n\\n# Objective combining N=2, N=3 (A=-10, 0), N=4 (A=-10,0,10)\\n\\ndef objective(p):\\n    # bounds by penalty: force Jx1>=0, Jx2_0>=0, hx>=0\\n    pen = 0.0\\n    if p[0] < 0: pen += 1e5*abs(p[0])\\n    if p[3] < 0: pen += 1e5*abs(p[3])\\n    if p[4] < 0: pen += 1e5*abs(p[4])\\n    if abs(p[5])>2: pen += 1e5*(abs(p[5])-2)**2\\n    # N=2\\n    e2 = gs_expectations(2, 0.0, p)\\n    err2 = 0.2*(e2['Mx']-N2['Mx'])**2 + (e2['Cxx']-N2['Cxx'])**2 + (e2['Cyy']-N2['Cyy'])**2 + (e2['Czz']-N2['Czz'])**2\\n    # N=3\\n    err3 = 0.0\\n    for A in [-10.0, 0.0]:\\n        e3 = gs_expectations(3, A, p)\\n        t = N3[A]\\n        err3 += 0.1*(e3['Mx']-t['Mx'])**2 + (e3['Cxx_nn']-t['Cxx_nn'])**2 + (e3['Cyy_nn']-t['Cyy_nn'])**2 + (e3['Czz_nn']-t['Czz_nn'])**2\\n    # N=4\\n    err4 = 0.0\\n    for A in [-10.0, 0.0, 10.0]:\\n        e4 = gs_expectations(4, A, p)\\n        t = N4[A]\\n        errA = 0.1*(e4['Mx']-t['Mx'])**2 + (e4['Cxx_nn']-t['Cxx_nn'])**2 + (e4['Cyy_nn']-t['Cyy_nn'])**2 + (e4['Czz_nn']-t['Czz_nn'])**2 + (e4['Cxx_nnn']-t['Cxx_nnn'])**2 + (e4['Cyy_nnn']-t['Cyy_nnn'])**2 + (e4['Czz_nnn']-t['Czz_nnn'])**2\\n        err4 += errA\\n    return err2 + err3 + err4 + pen\\n\\n# Perform bounded optimization via transform to unconstrained (soft penalty already used)\\ninits = [\\n    np.array([1.0, -0.1, 0.4, 0.5, 0.2, 0.2]),\\n    np.array([1.4, -0.2, 0.4, 0.6, 0.3, 0.1]),\\n    np.array([0.8, -0.3, 0.2, 0.2, 0.1, 0.3]),\\n]\\n\\nbest = None; best_val = 1e99\\nfor p0 in inits:\\n    res = minimize(objective, p0, method='Nelder-Mead', options={'maxiter':8000, 'xatol':1e-7, 'fatol':1e-7})\\n    if res.fun < best_val:\\n        best_val = res.fun\\n        best = res\\n\\np_opt = best.x\\npred2 = gs_expectations(2, 0.0, p_opt)\\npreds3 = {A: gs_expectations(3, A, p_opt) for A in [-10.0, 0.0]}\\npreds4 = {A: gs_expectations(4, A, p_opt) for A in [-10.0, 0.0, 10.0]}\\n\\nresult = {\\n    'opt_status': int(best.status),\\n    'opt_message': best.message,\\n    'params': {'Jx1': float(p_opt[0]), 'Jy1': float(p_opt[1]), 'Jz1': float(p_opt[2]), 'hx': float(p_opt[3]), 'Jx2_0': float(p_opt[4]), 'K': float(p_opt[5])},\\n    'N2_pred': pred2,\\n    'N3_pred_A-10': preds3[-10.0],\\n    'N3_pred_A0': preds3[0.0],\\n    'N4_pred_Am10': preds4[-10.0],\\n    'N4_pred_A0': preds4[0.0],\\n    'N4_pred_Ap10': preds4[10.0],\\n}\",\"result_label\":\"fit_constrained\"}", "call_id": "call_4M0t7h87KK6sekJHq6FME3Qv", "name": "execute_code", "type": "function_call", "id": "fc_68d35d225b6081958fabb95c400782c805ee3b1e64a904d8", "status": "completed"}, {"call_id": "call_4M0t7h87KK6sekJHq6FME3Qv", "type": "function_call_output", "output": "Saved the following results in the format <result_label>: <result>:\nfit_constrained_opt_status: 0\nfit_constrained_opt_message: Optimization terminated successfully.\nfit_constrained_params: Dictionary with keys with values:\nJx1: 5.769608374233687e-15\nJy1: -0.29685271612843606\nJz1: 0.4894109254420387\nhx: 0.7415435278540703\nJx2_0: 0.0019295714672211536\nK: 0.15398429744408032\n\nfit_constrained_N2_pred: Dictionary with keys with values:\nMx: 1.7670336308531789\nCxx: 1.0000000000000004\nCyy: -0.46839944156505836\nCzz: 0.4683994415650584\n\nfit_constrained_N3_pred_A-10: Dictionary with keys with values:\nMx: 1.7812993994846857\nCxx_nn: 0.8675420557279987\nCyy_nn: -0.18938142662242372\nCzz_nn: 0.7901422290786306\n\nfit_constrained_N3_pred_A0: Dictionary with keys with values:\nMx: 2.5941961476001376\nCxx_nn: 1.795757038040182\nCyy_nn: -0.8278950972898478\nCzz_nn: 0.874396741293021\n\nfit_constrained_N4_pred_Am10: Dictionary with keys with values:\nMx: 1.65447696043244\nCxx_nn: 0.6190015475239925\nCyy_nn: -0.1718686756252905\nCzz_nn: 1.13