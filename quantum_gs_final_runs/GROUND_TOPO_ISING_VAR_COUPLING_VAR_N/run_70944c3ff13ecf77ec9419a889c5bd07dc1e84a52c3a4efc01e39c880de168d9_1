 {\\n    'Mx': float(exp_N6_A0_Mx),\\n    'Cxx_nn': float(exp_N6_A0_Cxx_nn),\\n    'Cyy_nn': float(exp_N6_A0_Cyy_nn),\\n    'Czz_nn': float(exp_N6_A0_Czz_nn),\\n}\\n\\nsx = np.array([[0,1],[1,0]], dtype=complex)\\nsy = np.array([[0,-1j],[1j,0]], dtype=complex)\\nsz = np.array([[1,0],[0,-1]], dtype=complex)\\nI2 = np.eye(2, dtype=complex)\\n\\nfrom functools import reduce\\n\\ndef kronN(ops):\\n    return reduce(np.kron, ops)\\n\\n_cache_ops = {}\\n\\ndef site_ops(N):\\n    if N in _cache_ops: return _cache_ops[N]\\n    Sx = []; Sy = []; Sz = []\\n    Ids = [I2]*N\\n    for i in range(N):\\n        ops = list(Ids)\\n        ops[i] = sx; Sx.append(kronN(ops))\\n        ops[i] = sy; Sy.append(kronN(ops))\\n        ops[i] = sz; Sz.append(kronN(ops))\\n    _cache_ops[N]=(Sx,Sy,Sz)\\n    return Sx,Sy,Sz\\n\\n# H0 parameters p = [J1x, J1y, J1z, h, J2x]\\n\\ndef build_H0(N, p):\\n    J1x, J1y, J1z, h, J2x = p\\n    Sx,Sy,Sz = site_ops(N)\\n    d=2**N\\n    H = np.zeros((d,d), dtype=complex)\\n    # NN\\n    for i in range(N-1):\\n        H -= J1x*(Sx[i]@Sx[i+1])\\n        H -= J1y*(Sy[i]@Sy[i+1])\\n        H -= J1z*(Sz[i]@Sz[i+1])\\n    # field\\n    for i in range(N):\\n        H -= h*Sx[i]\\n    # NNN x-x\\n    for i in range(N-2):\\n        H -= J2x*(Sx[i]@Sx[i+2])\\n    return H,(Sx,Sy,Sz)\\n\\n\\ndef gs_expect(N, p):\\n    H,(Sx,Sy,Sz) = build_H0(N,p)\\n    vals,vecs = np.linalg.eigh(H)\\n    psi = vecs[:,0]\\n    res={'Mx': float(np.real(np.vdot(psi,sum(Sx)@psi)))}\\n    # NN sums\\n    Cxx=Cyy=Czz=0.0\\n    for i in range(N-1):\\n        Cxx += float(np.real(np.vdot(psi,(Sx[i]@Sx[i+1])@psi)))\\n        Cyy += float(np.real(np.vdot(psi,(Sy[i]@Sy[i+1])@psi)))\\n        Czz += float(np.real(np.vdot(psi,(Sz[i]@Sz[i+1])@psi)))\\n    res.update({'Cxx_nn':Cxx,'Cyy_nn':Cyy,'Czz_nn':Czz})\\n    if N>=4:\\n        Cxx2=Cyy2=Czz2=0.0\\n        for i in range(N-2):\\n            Cxx2 += float(np.real(np.vdot(psi,(Sx[i]@Sx[i+2])@psi)))\\n            Cyy2 += float(np.real(np.vdot(psi,(Sy[i]@Sy[i+2])@psi)))\\n            Czz2 += float(np.real(np.vdot(psi,(Sz[i]@Sz[i+2])@psi)))\\n        res.update({'Cxx_nnn':Cxx2,'Cyy_nnn':Cyy2,'Czz_nnn':Czz2})\\n    return res\\n\\n# Objective using N=2,4,6 at A=0\\n\\ndef objective(p):\\n    # soft bounds keep |p|<=5\\n    if np.any(np.abs(p)>5):\\n        return 1e6\\n    err=0.0\\n    e2=gs_expect(2,p)\\n    for k in ['Mx','Cxx_nn','Cyy_nn','Czz_nn']:\\n        # N=2 has only one NN bond results equal to N2's given 'Cxx' etc\\n        target = N2['Mx'] if k=='Mx' else N2[k.replace('_nn','')]\\n        err += (e2[k]-target)**2 * (0.2 if k=='Mx' else 1.0)\\n    e4=gs_expect(4,p)\\n    for k in e4:\\n        target = N4[k]\\n        w = 0.1 if k=='Mx' else 1.0\\n        err += w*(e4[k]-target)**2\\n    e6=gs_expect(6,p)\\n    for k in ['Mx','Cxx_nn','Cyy_nn','Czz_nn']:\\n        target = N6[k]\\n        w=0.05 if k=='Mx' else 0.5\\n        err += w*(e6[k]-target)**2\\n    return err\\n\\nbest=None; best_f=1e99\\ninits=[np.array([1.0,-0.2,0.4,0.5,0.2]), np.array([1.5,-0.3,0.5,0.8,0.1]), np.array([0.8,-0.5,0.3,0.3,0.0])]\\nfor p0 in inits:\\n    res=minimize(objective,p0,method='Nelder-Mead',options={'maxiter':8000,'xatol':1e-7,'fatol':1e-7})\\n    if res.fun<best_f:\\n        best=res; best_f=res.fun\\n\\np_opt=best.x\\npred2=gs_expect(2,p_opt)\\npred4=gs_expect(4,p_opt)\\npred6=gs_expect(6,p_opt)\\n\\nresult={'params':{'J1x':float(p_opt[0]),'J1y':float(p_opt[1]),'J1z':float(p_opt[2]),'h':float(p_opt[3]),'J2x':float(p_opt[4])},\\n        'pred_N2':pred2,'pred_N4':pred4,'pred_N6':pred6,'best_fun':float(best_f),'opt_status':int(best.status),'opt_message':best.message}\\n\",\"result_label\":\"fit_A0_base\"}", "call_id": "call_SJZMrkewzZsJRZdDl4BkKU9I", "name": "execute_code", "type": "function_call", "id": "fc_68d35ebce39881958f57546df5680a5b05ee3b1e64a904d8", "status": "completed"}, {"call_id": "call_SJZMrkewzZsJRZdDl4BkKU9I", "type": "function_call_output", "output": "Saved the following results in the format 