s[i]=sy; Sy.append(kronN(ops))\\n    ops[i]=sz; Sz.append(kronN(ops))\\n\\n# Build Hamiltonian for given K and A: H = -Jz * sum Sz_i Sz_{i+1} - h * sum Sx_i - A * K * sum (Sy_i Sy_{i+2} - Sz_i Sz_{i+2})\\n\\ndef H_of_A(A, K):\\n    H = np.zeros((2**N,2**N), dtype=complex)\\n    for i in range(N-1):\\n        H += -Jz * (Sz[i] @ Sz[i+1])\\n    for i in range(N):\\n        H += -h * Sx[i]\\n    for i in range(N-2):\\n        H += -A * K * (Sy[i] @ Sy[i+2] - Sz[i] @ Sz[i+2])\\n    return H\\n\\n# Compute observables\\n\\ndef obs(A,K):\\n    H=H_of_A(A,K)\\n    vals,vecs=np.linalg.eigh(H)\\n    psi=vecs[:,0]\\n    Mx=float(np.real(np.vdot(psi, (Sx[0]+Sx[1]+Sx[2]+Sx[3]) @ psi)))\\n    Cxx_nn=sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1])@psi))) for i in range(3))\\n    Cyy_nn=sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1])@psi))) for i in range(3))\\n    Czz_nn=sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1])@psi))) for i in range(3))\\n    Cxx_nnn=sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+2])@psi))) for i in range(2))\\n    Cyy_nnn=sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+2])@psi))) for i in range(2))\\n    Czz_nnn=sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+2])@psi))) for i in range(2))\\n    return {'Mx':Mx,'Cxx_nn':Cxx_nn,'Cyy_nn':Cyy_nn,'Czz_nn':Czz_nn,'Cxx_nnn':Cxx_nnn,'Cyy_nnn':Cyy_nnn,'Czz_nnn':Czz_nnn}\\n\\n# Fit K to minimize error vs all A\\n\\ndef objective(logK):\\n    K = np.exp(logK)\\n    err=0.0\\n    for A in Avals:\\n        e=obs(A,K)\\n        t=N4_targets[A]\\n        err += 0.2*(e['Mx']-t['Mx'])**2 + (e['Cxx_nn']-t['Cxx_nn'])**2 + (e['Cyy_nn']-t['Cyy_nn'])**2 + (e['Czz_nn']-t['Czz_nn'])**2 + (e['Cxx_nnn']-t['Cxx_nnn'])**2 + (e['Cyy_nnn']-t['Cyy_nnn'])**2 + (e['Czz_nnn']-t['Czz_nnn'])**2\\n    return err\\n\\nres=minimize(objective, x0=0.0, method='Nelder-Mead', options={'maxiter':2000,'xatol':1e-6,'fatol':1e-6})\\nlogK=res.x[0]\\nK=float(np.exp(logK))\\npreds={A:obs(A,K) for A in Avals}\\n\\nresult={'Jz':Jz,'h':h,'K':K,'preds':preds,'opt_status':int