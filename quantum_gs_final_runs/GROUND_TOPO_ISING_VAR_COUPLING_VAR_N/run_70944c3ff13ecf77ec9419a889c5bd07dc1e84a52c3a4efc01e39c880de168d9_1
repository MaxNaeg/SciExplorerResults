n = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))) for i in range(2))\\n        Cyy_nn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))) for i in range(2))\\n        Czz_nn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))) for i in range(2))\\n        return dict(Mx=Mx, Cxx_nn=Cxx_nn, Cyy_nn=Cyy_nn, Czz_nn=Czz_nn)\\n    if N==4:\\n        Mx = float(np.real(np.vdot(psi, sum(Sx) @ psi)))\\n        Cxx_nn = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))) for i in range(3))\\n        Cyy_nn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))) for i in range(3))\\n        Czz_nn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))) for i in range(3))\\n        Cxx_nnn = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+2]) @ psi))) for i in range(2))\\n        Cyy_nnn = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+2]) @ psi))) for i in range(2))\\n        Czz_nnn = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+2]) @ psi))) for i in range(2))\\n        return dict(Mx=Mx,Cxx_nn=Cxx_nn,Cyy_nn=Cyy_nn,Czz_nn=Czz_nn,Cxx_nnn=Cxx_nnn,Cyy_nnn=Cyy_nnn,Czz_nnn=Czz_nnn)\\n\\n# Objective combining N=2, N=3 (A=-10, 0), N=4 (A=-10,0,10)\\n\\ndef objective(p):\\n    # bounds by penalty: force Jx1>=0, Jx2_0>=0, hx>=0\\n    pen = 0.0\\n    if p[0] < 0: pen += 1e5*abs(p[0])\\n    if p[3] < 0: pen += 1e5*abs(p[3])\\n    if p[4] < 0: pen += 1e5*abs(p[4])\\n    if abs(p[5])>2: pen += 1e5*(abs(p[5])-2)**2\\n    # N=2\\n    e2 = gs_expectations(2, 0.0, p)\\n    err2 = 0.2*(e2['Mx']-N2['Mx'])**2 + (e2['Cxx']-N2['Cxx'])**2 + (e2['Cyy']-N2['Cyy'])**2 + (e2['Czz']-N2['Czz'])**2\\n    # N=3\\n    err3 = 0.0\\n    for A in [-10.0, 0.0]:\\n        e3 = gs_expectations(3, A, p)\\n        t = N3[A]\\n        err3 += 0.1*(e3['Mx']-t['Mx'])**2 + (e3['Cxx_nn']-t['Cxx_nn'])**2 + (e3['Cyy_nn']-t['Cyy_nn'])**2 + (e3['Czz_nn']-t['Czz_nn'])**2\\n    # N=4\\n    err4 = 0.0\\n    for A in [-10.0, 0.0, 10.0]:\\n        e4 = gs_expectations(4, A, p)\\n        t = N4[A]\\n        errA 