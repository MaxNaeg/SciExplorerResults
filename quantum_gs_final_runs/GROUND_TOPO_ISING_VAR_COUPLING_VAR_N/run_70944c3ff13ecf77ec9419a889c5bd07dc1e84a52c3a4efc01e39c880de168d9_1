omplex)\\n\\n# Build single-site ops for N spins\\n\\ndef site_ops(N):\\n    Sx = []\\n    Sy = []\\n    Sz = []\\n    for i in range(N):\\n        ops = [I2]*N\\n        ops[i] = sx\\n        Sx.append(kronN(ops))\\n        ops[i] = sy\\n        Sy.append(kronN(ops))\\n        ops[i] = sz\\n        Sz.append(kronN(ops))\\n    return Sx, Sy, Sz\\n\\n# Efficient Kronecker product builder\\nfrom functools import reduce\\n\\ndef kronN(ops):\\n    return reduce(np.kron, ops)\\n\\n# Hamiltonian builder with parameters p and A\\n# p = [Jx, Jy, Jz, hx, Jx2, Jy2_0, Jz2_0, alpha]\\n\\ndef build_H(N, A, p, cache={}):\\n    Jx, Jy, Jz, hx, Jx2, Jy2_0, Jz2_0, alpha = p\\n    key = ('ops', N)\\n    if key in cache:\\n        Sx, Sy, Sz = cache[key]\\n    else:\\n        Sx, Sy, Sz = site_ops(N)\\n        cache[key] = (Sx, Sy, Sz)\\n    H = np.zeros((2**N, 2**N), dtype=complex)\\n    # NN terms\\n    for i in range(N-1):\\n        H -= Jx * (Sx[i] @ Sx[i+1])\\n        H -= Jy * (Sy[i] @ Sy[i+1])\\n        H -= Jz * (Sz[i] @ Sz[i+1])\\n    # field\\n    for i in range(N):\\n        H -= hx * Sx[i]\\n    # NNN terms\\n    Jy2 = Jy2_0 + alpha * A\\n    Jz2 = Jz2_0 - alpha * A\\n    for i in range(N-2):\\n        H -= Jx2 * (Sx[i] @ Sx[i+2])\\n        H -= Jy2 * (Sy[i] @ Sy[i+2])\\n        H -= Jz2 * (Sz[i] @ Sz[i+2])\\n    return H, (Sx, Sy, Sz)\\n\\n# Observable calculators\\n\\ndef gs_expectations(N, A, p):\\n    H, (Sx, Sy, Sz) = build_H(N, A, p)\\n    vals, vecs = np.linalg.eigh(H)\\n    psi = vecs[:, 0]\\n    # Mx\\n    Mx_op = sum(Sx)\\n    Mx = np.real(np.vdot(psi, Mx_op @ psi))\\n    # NN\\n    Cxx_nn = Cyy_nn = Czz_nn = 0.0\\n    for i in range(N-1):\\n        Cxx_nn += np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))\\n        Cyy_nn += np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))\\n        Czz_nn += np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))\\n    # NNN\\n    Cxx_nnn = Cyy_nnn = Czz_nnn = 0.0\\n    for i in range(N-2):\\n        Cxx_nnn += np.real(np.vdot(psi, (Sx[i]@Sx[i+2]) @ psi))\\n        Cyy_nnn += np.real(np.vdot(psi, (Sy[i]@Sy[i+2]) @ psi))\\n        Czz_nnn += np.real(np.vdot(psi, (Sz[i]@Sz[i+2]) @ psi))\\n    return {\\n        'Mx': Mx,\\n        'Cxx_nn': Cxx_nn,\\n        'Cyy_nn': Cyy_nn,\\n        'Czz_nn': Czz_nn,\\n        'Cxx_nnn': Cxx_nnn,\\n        'Cyy_nnn': Cyy_nnn,\\n        'Czz_nnn': Czz_nnn,\\n    }\\n\\n# Objective function combining N=2 and N=4 across A\\n\\ndef objective(p):\\n    # add soft bounds to keep parameters reasonable\\n    penalty = 0.0\\n    if np.any(np.abs(p) > 5.0):\\n        penalty += 1e3 * np.sum((np.abs(p) - 5.0)[np.abs(p) > 5.0]**2)\\n    # N=2 check at A=0\\n    H2, (Sx2, Sy2, Sz2) = build_H(2, 0.0, p)\\n    vals2, vecs2 = np.linalg.eigh(H2)\\n    psi2 = vecs2[:, 0]\\n    Mx2 = np.real(np.vdot(psi2, (Sx2[0]+Sx2[1]) @ psi2))\\n    Cxx2 = np.real(np.vdot(psi2, (Sx2[0]@Sx2[1]) @ psi2))\\n    Cyy2 = np.real(np.vdot(psi2, (Sy2[0]@Sy2[1]) @ psi2))\\n    Czz2 = np.real(np.vdot(psi2, (Sz2[0]@Sz2[1]) @ psi2))\\n    err2 = (\\n        (Mx2 - N2_targets['Mx'])**2 * 0.2 +\\n        (Cxx2 - N2_targets['Cxx'])**2 +\\n        (Cyy2 - N2_targets['Cyy'])**2 +\\n        (Czz2 - N2_targets['Czz'])**2\\n    )\\n    # N=4 across A\\n    err4 = 0.0\\n    for A, t in N4_targets.items():\\n        e = gs_expectations(4, A, p)\\n        errA = (\\n            (e['Mx'] - t['Mx'])**2 * 0.1 +\\n            (e['Cxx_nn'] - t['Cxx_nn'])**2 +\\n            (e['Cyy_nn'] - t['Cyy_nn'])**2 +\\n            (e['Czz_nn'] - t['Czz_nn'])**2 +\\n            (e['Cxx_nnn'] - t['Cxx_nnn'])**2 +\\n            (e['Cyy_nnn'] - t['Cyy_nnn'])**2 +\\n            (e['Czz_nnn'] - t['Czz_nnn'])**2\\n        )\\n        err4 += errA\\n    return err2 + err4 + penalty\\n\\n# Initial guess from N=2 fit, with small NNNs, and alpha >0\\np0 = np.array([1.38, -0.12, 0.38, 0.50, 0.10, -0.10, 0.60, 0.05], dtype=float)\\n\\nres = optimize.minimize(objective, p0, method='Nelder-Mead', options={'maxiter': 2000, 'xatol':1e-6, 'fatol':1e-6})\\np_opt = res.x\\n\\n# Collect predictions\\npreds = {}\\nfor A in [-10.0, 0.0, 10.0]:\\n    preds[A] = gs_expectations(4, A, p_opt)\\n\\n# Also N=2\\nH2, (Sx2, Sy2, Sz2) = build_H(2, 0.0, p_opt)\\nvals2, vecs2 = np.linalg.eigh(H2)\\npsi2 = vecs2[:, 0]\\nMx2 = float(np.real(np.vdot(psi2, (Sx2[0]+Sx2[1]) @ psi2)))\\nCxx2 = float(np.real(np.vdot(psi2, (Sx2[0]@Sx2[1]) @ psi2)))\\nCyy2 = float(np.real(np.vdot(psi2, (Sy2[0]@Sy2[1]) @ psi2)))\\nCzz2 = float(np.real(np.vdot(psi2, (Sz2[0]@Sz2[1]) @ psi2)))\\n\\nresult = {\\n    'opt_status': int(res.status),\\n    'opt_message': res.message,\\n    'params': {\\n        'Jx': float(p_opt[0]), 'Jy': float(p_opt[1]), 'Jz': float(p_opt[2]), 'hx': float(p_opt[3]),\\n        'Jx2': float(p_opt[4]), 'Jy2_0': float(p_opt[5]), 'Jz2_0': float(p_opt[6]), 'alpha': float(p_opt[7])\\n    },\\n    'N2_pred': {'Mx': Mx2, 'Cxx': Cxx2, 'Cyy': Cyy2, 'Czz': Czz2},\\n    'N4_pred_Am10': preds[-10.0],\\n    'N4_pred_A0': preds[0.0],\\n    'N4_pred_Ap10': preds[10.0],\\n}\",\"result_label\":\"fit_N4_J1J2A\"}", "call_id": "call_q5a4AsAlHw7cRAAits0ihcks", "name": "execute_code", "type": "function_call", "id": "fc_68d3545551d48195a92a0ab48f97991405ee3b1e64a904d8", "status": "completed"}, {"call_id": "call_q5a4AsAlHw7cRAAits0ihcks", "type": "function_call_output", "output": "Saved the following results in the format <result_label>: <result>:\nfit_N4_J1J2A_opt_status: 0\nfit_N4_J1J2A_opt_message: Optimization terminated successfully.\nfit_N4_J1J2A_params: Dictionary with keys with values:\nJx: -0.5881411644007206\nJy: -0.32402868765398646\nJz: 0.3336315867912436\nhx: 0.7646543129819332\nJx2: 0.2326365890693336\nJy2_0: -0.015939241255979553\nJz2_0: -0.020696518476786628\nalpha: 3.329912713609765e-08\n\nfit_N4_J1J2A_N2_pred: Dictionary with keys with values:\nMx: 1.8373132764443056\nCxx: 0.9999999999999993\nCyy: -0.3950569339353145\nCzz: 0.39505693393531444\n\nfit_N4_J1J2A_N4_pred_Am10: Dictionary with keys with values:\nMx: 1.7989200111597448\nCxx_nn: -0.05013