 Sx is None:\\n        Sx, Sy, Sz = site_ops(N)\\n        cache[('ops', N)] = (Sx, Sy, Sz)\\n    H = np.zeros((2**N, 2**N), dtype=complex)\\n    for i in range(N-1):\\n        H -= Jx * (Sx[i] @ Sx[i+1])\\n        H -= Jy * (Sy[i] @ Sy[i+1])\\n        H -= Jz * (Sz[i] @ Sz[i+1])\\n    for i in range(N):\\n        H -= hx * Sx[i]\\n    Jy2 = Jy2_0 - eta*A\\n    Jz2 = Jz2_0 + eta*A\\n    for i in range(N-2):\\n        H -= Jx2 * (Sx[i] @ Sx[i+2])\\n        H -= Jy2 * (Sy[i] @ Sy[i+2])\\n        H -= Jz2 * (Sz[i] @ Sz[i+2])\\n    return H, (Sx, Sy, Sz)\\n\\n# Observables for N=2 and N=4\\n\\ndef gs_expectations(N, A, p):\\n    H, (Sx, Sy, Sz) = build_H(N, A, p)\\n    vals, vecs = np.linalg.eigh(H)\\n    psi = vecs[:, 0]\\n    res = {}\\n    if N==2:\\n        res['Mx'] = float(np.real(np.vdot(psi, (Sx[0]+Sx[1]) @ psi)))\\n        res['Cxx'] = float(np.real(np.vdot(psi, (Sx[0]@Sx[1]) @ psi)))\\n        res['Cyy'] = float(np.real(np.vdot(psi, (Sy[0]@Sy[1]) @ psi)))\\n        res['Czz'] = float(np.real(np.vdot(psi, (Sz[0]@Sz[1]) @ psi)))\\n    if N==4:\\n        res['Mx'] = float(np.real(np.vdot(psi, sum(Sx) @ psi)))\\n        res['Cxx_nn'] = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+1]) @ psi))) for i in range(3))\\n        res['Cyy_nn'] = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+1]) @ psi))) for i in range(3))\\n        res['Czz_nn'] = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+1]) @ psi))) for i in range(3))\\n        res['Cxx_nnn'] = sum(float(np.real(np.vdot(psi, (Sx[i]@Sx[i+2]) @ psi))) for i in range(2))\\n        res['Cyy_nnn'] = sum(float(np.real(np.vdot(psi, (Sy[i]@Sy[i+2]) @ psi))) for i in range(2))\\n        res['Czz_nnn'] = sum(float(np.real(np.vdot(psi, (Sz[i]@Sz[i+2]) @ psi))) for i in range(2))\\n    return res\\n\\n# Objective function with stronger emphasis on \u00b110\\n\\ndef objective(p):\\n    # Soft penalty to keep |p| <= 5\\n    if np.any(np.abs(p) > 5):\\n        return 1e6 + np.sum(np.abs(p))\\n    e2 = gs_expectations(2, 0.0, p)\\n    err2 =